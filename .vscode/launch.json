  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/tmpTest.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "DEBUG",
      "env": {
        "HTTPS_PORT": "8443",
        "SSL_KEY": "ssl.key",
        "SSL_CERTIFICATE": "ssl.crt",
      },
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\src\\index.js",
      "restart": true,
      "runtimeExecutable": "nodemon"
    }
  ]
}